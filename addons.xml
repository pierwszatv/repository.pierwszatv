<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.pierwszatv" name="Pierwsza TV" version="1.0.0" provider-name="piotrekcrash">
	<requires>
		<import addon="xbmc.python" version="2.14.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="main.py">
		<provides>video</provides>
	</extension>
	<!--<extension point="xbmc.service" library="service.py" start="startup" /> -->
	<extension point="xbmc.addon.metadata">
		<description>Plugin pozwalający na oglądanie kanałów telewizyjnych popularnego serwisu PierwszaTV.pl</description>
		<platform>all</platform>
		<summary lang="en"></summary>
	</extension>
</addon>
<addon id="repository.pierwszatv" name="PierwszaTV repository" version="1.0.0" provider-name="piotrekcrash">
	<extension point="xbmc.addon.repository" name="PierwszaTV Kodi Addon Repository">
		<info compressed="false">https://raw.github.com/pierwszatv/repository.pierwszatv/master/addons.xml</info>
		<checksum>https://raw.github.com/pierwszatv/repository.pierwszatv/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.github.com/pierwszatv/repository.pierwszatv/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Zainstaluj PierwszaTV Addon</summary>
		<description>Pobierz i zainstaluj addon PierwszaTV</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
